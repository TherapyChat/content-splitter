{
  "name": "@therapychat/content-splitter",
  "version": "0.2.0",
  "authors": [
    "TherapyChat.com team",
    "Alberto Fernández <alberto.fernandez@nc43tech.com>",
    "Rafael García <rafael.garcia@therapychat.com>"
  ],
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target wc --name content-splitter",
    "lint": "vue-cli-service lint",
    "version": "changelog-version && git add CHANGELOG.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TherapyChat/content-splitter.git"
  },
  "devDependencies": {
    "vue": "^2.5.17",
    "vue-mq": "^0.2.1",
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "vue-template-compiler": "^2.5.17",
    "changelog-version": "^1.0.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "main": "dist/content-splitter.min.js",
  "browser": "dist/content-splitter.min.js",
  "files": [
    "dist/**/*"
  ]
}
